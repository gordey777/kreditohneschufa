#footer .container {
    padding:0 0 50px 0;
}


#footer .menu {
    padding:50px 0 50px 0;
    text-align:center;
}

#footer .menu ul {
    margin:0;
    padding:0;
    list-style:none;
}

#footer .menu li {
    display:inline-block;
    vertical-align:top;
}

#footer .menu li:after {
    content:'|';
    margin:0 10px 0 14px;
    position:relative;
    top:-1px;
    font-family: 'Merriweather Sans', sans-serif;
    font-size:13px;
    font-weight:300;
    color:#DDB103;
}

#footer .menu li:last-child:after {display:none;}

#footer .menu a {
    font-size:18px;
    color:#F9F9F9;
    text-decoration:none;
}

#footer .menu .current-menu-item a {color:#DDB103;}

#footer .menu a:hover {color:#DDB103}


#footer .social {
    margin:0 0 0 25px;
}

#footer .social ul {
    margin:0;
    padding:0;
    list-style:none;
}

#footer .social li {
    width:40px;
    height:40px;
    margin:0 4px 0 0;
    display:inline-block;
    vertical-align:top;
}

#footer .social li a {
    width:40px;
    height:40px;
    display:block;
    opacity:0.75;
}

#footer .social li:nth-child(1) a {
    background: url('../images/social/facebook.png') 0 0 no-repeat;
    background-size:40px 40px;
}

#footer .social li:nth-child(2) a {background: url('../images/social/twitter.png') 0 0 no-repeat;background-size:40px 40px;
}

#footer .social li:nth-child(3) a {background: url('../images/social/dribbble.png') 0 0 no-repeat;background-size:40px 40px;}
#footer .social li:nth-child(4) a {background: url('../images/social/instagram.png') 0 0 no-repeat;background-size:40px 40px;}



#footer .social li a:hover {opacity:0.95;}


#footer .logo a {
    background: url('../images/logo-footer.png') 50% 0 no-repeat;
    width:200px;
    height:78px;
    display:block;
    margin:0 0 0 -100px;
    position:absolute;
    top:100px;
    left:50%;
    opacity:0.60;
}

#footer .logo span {display:none;}

#footer .top {
    position:absolute;
    top:115px;
    right:25px;
}

#footer .top a {
    background: url('../images/footer-top.png') 0 0 no-repeat;
    width:50px;
    height:50px;
    background-size:50px 50px;
    opacity:0.65;
    display:block;
}

#footer .top a:hover {opacity:0.95;}


#footer .copyright {
    background:#333333;
    height:50px;
    line-height:50px;
    font-family: 'Open Sans', sans-serif;
    font-size:15px;
    font-weight:300;
    color:rgba(255, 255, 255, 0.50);
    text-align:center;
}

#header .menu-link {display:none;}

.entry,#content .news .block .text {
    background: #F6F6F6;
    padding: 20px;
}
.entry p,#content .news .block .text p{
    font-size: 18px;
    line-height: 24px;
    margin-top: 0;
    margin-bottom: 22px;
    text-align: justify;
}

.entry img,#content .news .block .text img:not(.wp-image-426){
    margin-top: 4px;
    margin-bottom: 20px;
}

 .entry img.wp-image-402 {
    margin-top: 25px;
 }
img.wp-image-426.alignright {
    margin-top: 26px;
    margin-bottom: 14px;
    margin-right: 18px;
 }

#content .page .entry ul.nav {
    margin-left: 0;
    margin-top: 26px;
}
#content .page .entry ul.tab-content{
    margin-left: 0;
    margin-top: 0;
}
/*#content .page .entry ul,#content .tabbable div:last-child ul, #content .page .entry ul{
margin-left: 0;
}
#content .page .entry ul{
    margin-top: 16px;
    }*/

    #content .page .entry blockquote {
        margin: 0 0 20px 0px !important;
    }

    #content .page .text blockquote {
        margin: 0 !important;
    }
    #content .page .entry ul {
        font-size: 18px;
    }
    p::selection,
    a::selection,
    strong::selection,
    span::selection {
        color: #000;
        background-color: #ccc;
    }
    #content .page h1 {
        text-align: right;
    }

    blockquote * {
        margin: 0 !important;
    }

.entry ul li {
    margin-bottom: 20px;
}
@media all and (max-width: 320px) {
   #content .page .entry ul {
    font-size: 12px;
    font-weight: bold; 
   }
   .entry p, #content .news .block .text p {
    font-size: 13px;
}
}
#header,nav, a, img, li, ul{
  transition: all 0.5s;
  -moz-transition: all 0.5s; /* Firefox 4 */
  -webkit-transition: all 0.5s; /* Safari and Chrome */
  -o-transition: all 0.5s; /* Opera */
}