#slider .item {
    height:450px !important;
    position:relative;
}

#slider .item.one {
    background: url('../images/slider/slide-1.jpg') 50% 50% no-repeat;
    background-size:auto 450px;
}

#slider .item.two {background: url('../images/slider/slide-2.jpg') 50% 50% no-repeat;}
#slider .item.three {background: url('../images/slider/slide-3.jpg') 50% 50% no-repeat;}

#slider .item:before {
    content:'';
    background:rgba(0, 0, 0, 0.15);
    width:100%;
    height:100%;
    display:block;
    position:absolute;
    top:0;
    left:0;
}

#slider .owl-controls {
    width:100%;
    display:block;
    position:absolute;
    bottom:0;
    left:0;
}

#slider .owl-prev {
    background: url('../images/slider-arrow-left.png') 0 0 no-repeat;
    width:65px;
    height:65px;
    display:block;
    position:absolute;
    bottom:275px;
    left:25px;
    text-indent: -99999px;
    opacity:0.75;
}

#slider .owl-next {
    background: url('../images/slider-arrow-right.png') 0 0 no-repeat;
    width:65px;
    height:65px;
    display:block;
    position:absolute;
    bottom:275px;
    right:25px;
    text-indent: -99999px;
    opacity:0.75;
}

#slider .owl-prev:hover,
#slider .owl-next:hover {opacity:0.95;}

#slider .owl-pagination {
    width:100%;
    height:40px;
    position:absolute;
    bottom:0px;
    left:0;
    z-index:2;
    text-align:center;
}

#slider .owl-pagination .owl-page {
    background:#FFFFFF;
    width:15px;
    height:15px;
    margin:0 5px;
    display:inline-block;
    vertical-align:middle;
}

#slider .owl-pagination .owl-page:hover {background:#DDB103;}

#slider .owl-pagination .owl-page.active {
    background:#DDB103;
    width:22px;
    height:22px;
}

#slider ul {
    background:rgba(0, 0, 0, 0.32);
    width:100%;
    margin:0;
    padding:15px 0 35px 0;
    display:block;
    position:absolute;
    top:145px;
    left:0;
    list-style:none;
    text-align:center;
}

#slider ul li {
    margin:0 0 -4px 0;
    font-family: 'Open Sans', sans-serif;
    font-size:40px;
    font-weight:400;
    color:rgba(255, 255, 255, 1);
    text-shadow: 1px 1px 0px rgba(0, 0, 0, 0.15);
}

#slider ul li strong {
    font-weight:700;
    color:#DDB103;
}

#slider .button a {
    background:#DDB103;
    height:50px;
    line-height:50px;
    padding:0 35px;
    display:inline-block;
    font-size:30px;
    font-weight:400;
    color:#FFFFFF;
    text-decoration:none;
    text-transform: uppercase;
    position:relative;
}
#header .button a {
    background: #DDB103;
    height: 50px;
    line-height: 50px;
    padding: 0 35px;
    display: none;
    font-size: 21px;
    font-weight: 400;
    color: #FFFFFF;
    text-decoration: none;
    text-transform: uppercase;
    position: relative;
    float: right;
    margin: 23px 0px 0px 7px;
}
#header .button a:hover {
    background:#F2BE04;
    color:rgba(0, 0, 0, 0.65);
    text-shadow:none;
}
