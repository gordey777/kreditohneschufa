#header .container {border-top:solid 5px #DDB103;}

#header .logo {
    float:left;
    margin:10px 0 0 0;
}

#header .logo a {
    background: url('../images/logo.png') 0 0 no-repeat;
    background-size: 100%;
    width:169px;
    height:73px;
    display:block;
}
#header .logo_scroll a {
    background: url('../images/logo.png') 0 0 no-repeat;
    background-size: 100%;
    width:110px;
    height:50px;
    display:block;
}

#header .logo span {display:none;}

#header .menu {
    float:right;
    margin:33px 0 0 0;
}

#header .menu ul {
    margin:0;
    padding:0;
    list-style:none;
}

#header .menu li {
    display:inline-block;
    vertical-align:top;
}

#header .menu li:after {
    content:'/';
    margin:0 10px 0 14px;
    position:relative;
    top:-1px;
    font-family: 'Merriweather Sans', sans-serif;
    font-size:15px;
    font-weight:300;
    color:#DDB103;
}

#header .menu li:last-child:after {display:none;}

#header .menu a {
    font-family: 'Francois One', sans-serif;
    font-size:21px;
    color:#3F3F3F;
    text-decoration:none;
}

#header .menu .current-menu-item a,
#header .menu .current-menu-parent a {color:#DDB103;}

#header .menu a:hover {color:#DDB103}
#content {
margin:100px 0 0 0;
}
#slider {
    width:100%;
    max-width:1150px;
    height:450px;
    margin:0 auto;
    position:relative;
    overflow:hidden;
}

