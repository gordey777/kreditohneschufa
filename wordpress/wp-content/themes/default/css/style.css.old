    width:100%;
    max-width:570px;
    height:160px;
    display:inline-block;
    vertical-align:top;
    text-align:center;
}

#content .list .text .sub-block {
    width:100%;
    height:100%;
    display:table;
    white-space: normal;
}

#content .list .text .sub-block div{
    padding:0 20px;
}

#content .list .text h4 {
    margin:0;
}

#content .list .text p {
    margin:10px 0 0 0;
    font-size:14px;
    line-height:17px;
    color:rgba(255, 255, 255, 0.75);
}

#content .list .text a {
    width:100%;
    height:100%;
    display:table-cell;
    vertical-align:middle;
    font-family: 'Oswald', sans-serif;
    font-size:35px;
    color:rgba(255, 255, 255, 0.95);
    text-decoration:none;
}
#content .list .text a:hover{
    opacity: 1;
    color: rgba(0, 0, 0, 0.65);
}

