    background:#F2BE04;
    color:rgba(0, 0, 0, 0.65);
    text-shadow:none;
}

#slider .button a:hover:before {border-bottom: 50px solid #F2BE04;}
#slider .button a:hover:after {border-top: 50px solid #F2BE04; }

#content .table {
    margin:40px 0 0 0;
}


#content .table h2 {
    margin:0 0 35px 0;
    font-family: 'Francois One', sans-serif;
    font-size:45px;
    text-align:center;
    text-transform: uppercase;
    font-weight:400;
}

#content .table table {
    width:100%;
    border-collapse: collapse;
}

#content .table td {
    background:#F9F9F9;
    width:144.3px;
    padding:10px 7px;
    font-size:15px;
    text-align:center;
    border:solid 1px #E5E5E5;
}

#content .table td img {
    width:100% !important;
    max-width:125px !important;
    height:auto !important;
}

#content .table td.space {
    padding:0;
    position:relative;
}

#content .table td.space:before {
    content:'';
    background:#FFFFFF url('../images/logo-table.png') 50% 50% no-repeat;
    width:100%;
    height:100%;
    display:block;
    position:absolute;
    top:0;
    left:0;
    border-right:solid 1px #E5E5E5 !important;
}

#content .table td:first-child {
    width:186px;
    font-weight:700;
}

#content .table tr:first-child td {
    background:#F2F2F2 !important;
    font-weight:700;
}

#content .table tr:nth-child(2) td:first-child,
#content .table tr:nth-child(3) td:first-child {
    background:none !important;
    font-weight:400;
}

#content .table tr:nth-child(3) td:first-child {background:#F9F9F9 !important;}

#content .table tr:nth-child(2n+2) td {background:#FFFFFF;}

#content .table tr:nth-child(2) td:first-child {
    width:100px !important;}

    #content .table tr td.button a {
        background:#DDB103;
        height:24px;
        line-height:26px;
        display:inline-block;
        padding:0 6px;
        font-size:13px;
        font-weight:300;
        text-decoration:none;
        color: #FFFFFF;
        text-transform: uppercase;
    }

    #content .table tr td.button a:hover {
        background:#663F05;
        color: #DDB103;
    }

    #content .list {
        margin:40px 0 0 0;
    }

    #content .list h2 {
        margin:0 0 15px 0;
        font-family: 'Francois One', sans-serif;
        font-size:45px;
        text-align:center;
        text-transform: uppercase;
        font-weight:400;
    }

    #content .list .info {
        margin:0 0 35px 0;
        font-size:15px;
        color:#727272;
        line-height:24px;
    }

    #content .list .info p:first-child {margin-top:0;}
    #content .list .info p:last-child {margin-bottom:0;}

    #content .list .container {
        width:100%;
        max-width:1140px;
        padding:0;
    }

    #content .list .block {
        background:#DDB103;
        margin:0 0 10px 0;
        position:relative;
        white-space: nowrap;
    }

    #content .list .block:last-child {margin:0 0 0 0;}

